// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["fullTextSearch"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Token {
  id Int @id @default(autoincrement()) 
  tokenName String @db.VarChar(255)
  whitelistWallets String[] @db.VarChar(255)
  tokenAddress String @db.VarChar(255)
  coingecko String @db.VarChar(255)
  month String[] @db.VarChar(255)
  m1 String[] @db.VarChar(255)
  m2 String[] @db.VarChar(255)
  m3 String[] @db.VarChar(255)
  m4 String[] @db.VarChar(255)
  m5 String[] @db.VarChar(255)
  m6 String[] @db.VarChar(255)
  m7 String[] @db.VarChar(255)
  m8 String[] @db.VarChar(255)
  m9 String[] @db.VarChar(255)
  m10 String[] @db.VarChar(255)
  m11 String[] @db.VarChar(255)
  m12 String[] @db.VarChar(255)
  m13 String[] @db.VarChar(255)
  m14 String[] @db.VarChar(255)
  m15 String[] @db.VarChar(255)
  tge String[] @db.VarChar(255)
}

model easytokens {
  id Int @id @default(autoincrement()) 
  tokenName String @db.VarChar(255)
  whitelistWallets String[] @db.VarChar(255)
  tokenAddress String @db.VarChar(255)
  coingecko String @db.VarChar(255)
  month String[] @db.VarChar(255)
  m1 String[] @db.VarChar(255)
  m2 String[] @db.VarChar(255)
  m3 String[] @db.VarChar(255)
  m4 String[] @db.VarChar(255)
  m5 String[] @db.VarChar(255)
  m6 String[] @db.VarChar(255)
  m7 String[] @db.VarChar(255)
  m8 String[] @db.VarChar(255)
  m9 String[] @db.VarChar(255)
  m10 String[] @db.VarChar(255)
  m11 String[] @db.VarChar(255)
  m12 String[] @db.VarChar(255)
  m13 String[] @db.VarChar(255)
  m14 String[] @db.VarChar(255)
  m15 String[] @db.VarChar(255)
  tge String[] @db.VarChar(255)
  abbrev String @db.VarChar(255)
}